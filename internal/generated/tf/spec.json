									}
								}
							]
						}
					},
					{
						"name": "organization_id",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The GUID4 ID of the organization.\n"
						}
					},
					{
						"name": "project_id",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The GUID4 ID of the project."
						}
					}
				]
			}
		}
	],
